<?xml version="1.0" encoding="UTF-8" ?>


<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--    objects=beans, dependencies-->


    <bean class="com.substring.concepts.core.Engine" name="engine">
        <property name="companyName" value="Ford"></property>
    </bean>


    <bean class="com.substring.concepts.core.Car" id="car" autowire="byType">
        <property name="name" value="TataSafari"></property>

<!--        <property name="engine" ref="engine"></property>-->

    </bean>


</beans>